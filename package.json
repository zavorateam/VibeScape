{
  "name": "vibescape",
  "version": "0.9.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "ru.zavorateam.vibescape",
    "productName": "VibeScape",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png"
    }
  },
  "devDependencies": {
    "electron": "^33.4.11",
    "electron-builder": "^25.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zavorateam/VibeScape.git"
  },
